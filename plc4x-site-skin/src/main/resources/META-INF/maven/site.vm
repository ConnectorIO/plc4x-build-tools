#foreach( $author in $authors )
      <meta name="author" content="$author" />
#end
#if ( $documentDate )
    <meta name="date" content="$documentDate" />
#end
#if ( $locale )
    <meta http-equiv="Content-Language" content="$locale.language" />
#end
#if( $decoration.body.head )
